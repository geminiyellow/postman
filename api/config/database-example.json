{
    "dialect": "mysql",
    "database": "jianxin",
    "username": "username",
    "password": "password",
    "host": "localhost",

    "port": 3306,
    "protocol": "tcp",
    "maxConcurrentQueries": 50,
    "dialectOptions": {},
    "storage": ":memory:",
    "omitNull": false,
    "native": false,
    "define": {
